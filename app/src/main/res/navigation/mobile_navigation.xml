<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.example.myapplication.ui.home.Fragment_Base"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_base" />

    <activity
        android:id="@+id/nav_gallery"
        android:name="com.example.myapplication.ListaActivity"
        android:label="Preferiti"
        tools:layout="@layout/activity_lista" />

    <activity
        android:id="@+id/nav_slideshow"
        android:name="com.example.myapplication.ui.fragment_utente.Fragment_IMieiEventi"
        android:label="I tuoi eventi" />

    <activity
        android:id="@+id/nav_tools"
        android:name="com.example.myapplication.ActivityMappa"
        android:label="@string/menu_tools"
        tools:layout="@layout/fragment_mappa" />

    <activity
        android:id="@+id/nav_send"
        android:name="com.example.myapplication.ActivityNotifiche"
        android:label="Notifiche" />
</navigation>